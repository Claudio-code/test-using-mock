{
    "name": "claudio/test-using-mock",
    "authors": [
        {
            "name": "claudio-code",
            "email": "claudio-code@outlook.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "php-cs-fixer": "php-cs-fixer fix .",
        "run-all-tests": "vendor/bin/phpunit tests"
    }
}
